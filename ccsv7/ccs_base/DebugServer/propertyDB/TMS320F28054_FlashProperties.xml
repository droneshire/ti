	<!-- TMS320F28054 Property Definitions -->
	<property id="FlashOSCCLK">
		<target>TMS320C28XX</target>
		<name>OSCCLK (MHz):</name>
		<valueType>Numeric</valueType>
		<defaultValue>10</defaultValue>
		<readOnly />
	</property>
	
	<property id="FlashCLKINDV">
		<target>TMS320C28XX</target>
		<name>CLKINDIV: </name>
		<valueType>String</valueType>
		<defaultValue>2</defaultValue>
		<readOnly />
	</property>
	
	<property id="FlashPLLCR">
		<target>TMS320C28XX</target>
		<name>PLLCR Value:</name>
		<valueType>String</valueType>
		<defaultValue>12</defaultValue>
		<readOnly />
	</property>

	<property id="RAMSelectOptionText">
		<target>TMS320C28XX</target>
		<name>RAM Block Selection for Flash Operations (Must Reside in Same Zone as Flash Being Programmed)
[Note: the available options are dependent on the device you are using/available RAM sections on the device]</name>
		<valueType>Text</valueType>
	</property>
	
	<property id="Z1RAMSelectOptionSelect">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>StringList</valueType>
		<values>
			<value>RAML0+RAML1 (0x8000-0x8BFF)</value>
			<value>RAML0+RAML2 (0x8000-0x87FF,0x8C00-0x8FFF)</value>
			<value>RAML3 (0x9000-0x9FFF)</value>
		</values>
		<defaultValue>RAML0+RAML1 (0x8000-0x8BFF)</defaultValue>
	</property>
	
