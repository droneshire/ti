<?xml version="1.0" encoding="UTF-8"?>
<PropertyList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!-- Nodes -->
	<node id="FlashProgrammerNode">
		<name>Flash Settings</name>
		<children>
			<child>FlashSettingsGroup</child>
			<child>FlashDownloadSettingGroup</child>
			<child>FlashRangeGroup</child>
			<child>FlashPerformanceGroup</child>
			<child>FlashEraseGroup</child>
			<child>FlashBlankCheckGroup</child>
			<child>FlashChecksumGroup</child>
			<child>FlashC28SecurityGroup</child>
		</children>
	</node>

	<!-- Groups -->
	<group id="FlashSettingsGroup">
		<name>Clock/PLL Settings</name>
		<children>
			<child>FlashOSCCLK</child>
			<child>FlashSPLLIMULT</child>
			<child>FlashSPLLFMULT</child>
			<child>FlashSYSDIVSEL</child>
		</children>
	</group>
	
	<group id="FlashDownloadSettingGroup">
		<name>Download Settings</name>
		<children>
			<child>FlashDownloadSetting</child>
			<child>FlashBlankCheckToggle</child>
			<child>FlashVerifySetting</child>
			<child>FlashAutoECCSetting</child>
		</children>
	</group>
	
	<group id="FlashRangeGroup">
		<name>Range Avoidance Settings</name>
		<children>
			<child>FlashRangeText</child>
			<child>FlashRangeToggle</child>
			<child>FlashRangeAddr</child>
		</children>
	</group>
	
	<group id="FlashPerformanceGroup">
		<name>Performance Settings</name>
		<children>
			<child>FlashCombineText</child>
			<child>FlashCombineText2</child>
			<child>FlashAlignCombineToggle</child>
			<child>FlashAlignmentSize</child>
		</children>
	</group>
	
	<group id="FlashEraseGroup">
		<name>Erase Settings</name>
		<children>
			<child>FlashEraseSelection</child>
			<child>FlashBank0</child>
			<child>FlashEraseButton</child>
		</children>
	</group>
	
	<group id="FlashBank0">
		<name>Flash Bank 0</name>
		<children>
			<child>FlashC28Bank0Sector0</child>
			<child>FlashC28Bank0Sector1</child>
			<child>FlashC28Bank0Sector2</child>
			<child>FlashC28Bank0Sector3</child>
			<child>FlashC28Bank0Sector4</child>
			<child>FlashC28Bank0Sector5</child>
			<child>FlashC28Bank0Sector6</child>
			<child>FlashC28Bank0Sector7</child>
			<child>FlashC28Bank0Sector8</child>
			<child>FlashC28Bank0Sector9</child>
			<child>FlashC28Bank0Sector10</child>
			<child>FlashC28Bank0Sector11</child>
			<child>FlashC28Bank0Sector12</child>
			<child>FlashC28Bank0Sector13</child>
			<child>FlashC28Bank0Sector14</child>
			<child>FlashC28Bank0Sector15</child>
			<child>FlashEraseToggle0Button</child>
		</children>
	</group>
	
	<group id="FlashBlankCheckGroup">
		<name>Blank Check Settings</name>
		<children>
			<child>FlashBlankCheckButton</child>
		</children>
	</group>
			
	<group id="FlashChecksumGroup">
		<name>Checksum</name>
		<children>
			<child>FlashCRCMainValue</child>
			<child>FlashCRCECCValue</child>
			<child>FlashCRCOTPValue</child>
			<child>FlashCRCOTPECCValue</child>
			<child>FlashCalChecksumButton</child>
		</children>
	</group>
	
	<group id="FlashC28SecurityGroup">
		<name>Security Settings</name>
		<children>
			<child>SecurityText</child>
			<child>FlashSecurityZ1B0Group</child>
			<child>FlashSecurityZ2B0Group</child>
			<child>FlashSecurityAutoFillButton</child>
		</children>
	</group>
	
	<group id="FlashSecurityZ1B0Group">
		<name>Zone 1, Bank 0</name>
		<children>
			<child>Z1B0LINKPOINTERGroup</child>
			<child>Z1B0GPREGGroup</child>
			<child>Z1B0OTPSECLOCKGroup</child>
			<child>Z1B0OTPBOOTCTRLGroup</child>
			<child>Z1B0CSMPSWDGroup</child>
			<child>Z1B0GRABGroup</child>
			<child>Z1B0EXEONLYGroup</child>
			<child>Z1B0ProgramAllButton</child>
		</children>
	</group>
	
	<group id="Z1B0LINKPOINTERGroup">
		<name>LINKPOINTER</name>
		<children>
			<child>Z1B0LINKPOINTER1</child>
			<child>Z1B0LINKPOINTER2</child>
			<child>Z1B0LINKPOINTER3</child>
			<child>Z1B0LINKPOINTERButton</child>
		</children>
	</group>
	
	<group id="Z1B0GPREGGroup">
		<name>GPREG</name>
		<children>
			<child>Z1B0OTPGPREG1</child>
			<child>Z1B0OTPGPREG2</child>
			
			<child>Z1B0OTPGPREGButton</child>
		</children>
	</group>
	
	<group id="Z1B0OTPSECLOCKGroup">
		<name>OTPSECLOCK</name>
		<children>
			<child>Z1B0PSWDLOCK</child>
			<child>Z1B0CRCLOCK</child>
			<child>Z1B0OTPSECLOCKButton</child>
		</children>
	</group>
	
	<group id="Z1B0OTPBOOTCTRLGroup">
		<name>OTPBOOTCTRL/GPREG3</name>
		<children>
			<child>Z1B0OTPGPREG3</child>
			<child>Z1B0OTPBOOTCTRL</child>
			<child>Z1B0OTPBOOTCTRLButton</child>
		</children>
	</group>
	
	<group id="Z1B0CSMPSWDGroup">
		<name>CSMPSWD</name>
		<children>
			<child>Z1B0CSMPSWD0</child>
			<child>Z1B0CSMPSWD1</child>
			<child>Z1B0CSMPSWD2</child>
			<child>Z1B0CSMPSWD3</child>
			<child>Z1B0CSMPSWButton</child>
			<child>Z1B0LockButton</child>
			<child>Z1B0UnlockButton</child>
		</children>
	</group>
	
	<group id="Z1B0GRABGroup">
		<name>GRABSECT/RAM</name>
		<children>
			<child>Z1B0GRABSECT</child>
			<child>Z1B0GRABRAM</child>
			<child>Z1B0GRABButton</child>
		</children>
	</group>
	
	<group id="Z1B0EXEONLYGroup">
		<name>EXEONLYSECT/RAM</name>
		<children>
			<child>Z1B0EXEONLYSECT</child>
			<child>Z1B0EXEONLYRAM</child>
			<child>Z1B0EXEONLYButton</child>
		</children>
	</group>
	
	<group id="FlashSecurityZ2B0Group">
		<name>Zone 2, Bank 0</name>
		<children>
			<child>Z2B0LINKPOINTERGroup</child>
			<child>Z2B0GPREGGroup</child>
			<child>Z2B0OTPSECLOCKGroup</child>
			<child>Z2B0OTPBOOTCTRLGroup</child>
			<child>Z2B0CSMPSWDGroup</child>
			<child>Z2B0GRABGroup</child>
			<child>Z2B0EXEONLYGroup</child>
			<child>Z2B0ProgramAllButton</child>
		</children>
	</group>
	
	<group id="Z2B0LINKPOINTERGroup">
		<name>LINKPOINTER</name>
		<children>
			<child>Z2B0LINKPOINTER1</child>
			<child>Z2B0LINKPOINTER2</child>
			<child>Z2B0LINKPOINTER3</child>
			<child>Z2B0LINKPOINTERButton</child>
		</children>
	</group>
	
	<group id="Z2B0GPREGGroup">
		<name>GPREG</name>
		<children>
			<child>Z2B0OTPGPREG1</child>
			<child>Z2B0OTPGPREG2</child>
			
			<child>Z2B0OTPGPREGButton</child>
		</children>
	</group>
	
	<group id="Z2B0OTPSECLOCKGroup">
		<name>OTPSECLOCK</name>
		<children>
			<child>Z2B0PSWDLOCK</child>
			<child>Z2B0CRCLOCK</child>
			<child>Z2B0OTPSECLOCKButton</child>
		</children>
	</group>
	
	<group id="Z2B0OTPBOOTCTRLGroup">
		<name>OTPBOOTCTRL/GPREG3</name>
		<children>
			<child>Z2B0OTPGPREG3</child>
			<child>Z2B0OTPBOOTCTRL</child>
			<child>Z2B0OTPBOOTCTRLButton</child>
		</children>
	</group>
	
	<group id="Z2B0CSMPSWDGroup">
		<name>CSMPSWD</name>
		<children>
			<child>Z2B0CSMPSWD0</child>
			<child>Z2B0CSMPSWD1</child>
			<child>Z2B0CSMPSWD2</child>
			<child>Z2B0CSMPSWD3</child>
			<child>Z2B0CSMPSWButton</child>
			<child>Z2B0LockButton</child>
			<child>Z2B0UnlockButton</child>
		</children>
	</group>
	
	<group id="Z2B0GRABGroup">
		<name>GRABSECT/RAM</name>
		<children>
			<child>Z2B0GRABSECT</child>
			<child>Z2B0GRABRAM</child>
			<child>Z2B0GRABButton</child>
		</children>
	</group>
	
	<group id="Z2B0EXEONLYGroup">
		<name>EXEONLYSECT/RAM</name>
		<children>
			<child>Z2B0EXEONLYSECT</child>
			<child>Z2B0EXEONLYRAM</child>
			<child>Z2B0EXEONLYButton</child>
		</children>
	</group>
	
	<!-- Properties -->	
	<property id="FlashOSCCLK">
		<target>TMS320C28XX</target>
		<name>OSCCLK (MHz)(INTOSC2)</name>
		<valueType>Numeric</valueType>
		<defaultValue>10</defaultValue>
		<disableIf>
			<target>TMS320C28XX</target>
		</disableIf>
	</property>
	
	<property id="FlashSPLLIMULT">
		<target>TMS320C28XX</target>
		<name>System PLLCR Integer Multiplier (SPLLIMULT)</name>
		<valueType>Numeric</valueType>
		<defaultValue>20</defaultValue>
		<disableIf>
			<target>TMS320C28XX</target>
		</disableIf>
	</property>
	
	<property id="FlashSPLLFMULT">
		<target>TMS320C28XX</target>
		<name>System PLLCR Fractional Multiplier (SPLLFMULT)</name>
		<valueType>StringList</valueType>
		<values>
			<value>0 [0]</value>
			<value>1 [0.25]</value>
			<value>2 [0.5]</value>
			<value>3 [0.75]</value>
		</values>
		<defaultValue>0 [0]</defaultValue>
		<disableIf>
			<target>TMS320C28XX</target>
		</disableIf>
	</property>
	
	<property id="FlashSYSDIVSEL">
		<target>TMS320C28XX</target>
		<name>System Clock Divide Select (SYSDIVSEL)</name>
		<valueType>StringList</valueType>
		<values>
			<value>0 [/1]</value>
			<value>1 [/2]</value>
			<value>2 [/4]</value>
			<value>3 [/6]</value>
			<value>4 [/8]</value>
		</values>
		<defaultValue>1 [/2]</defaultValue>
		<disableIf>
			<target>TMS320C28XX</target>
		</disableIf>
	</property>
	
	<property id="FlashAutoECCSetting">
		<target>TMS320C28XX</target>
		<name>Auto ECC Generation</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
	</property>
	
	<property id="FlashRangeText">
		<target>TMS320C28XX</target>
		<name>If range option is enabled, the given range(s) will be avoided during Flash programming.
Note: Please list multiple ranges in ascending order separated by a comma</name>
		<valueType>Text</valueType>
	</property>
	
	<property id="FlashRangeToggle">
		<target>TMS320C28XX</target>
		<name>Enable Range Avoidance</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
	</property>
	
	<property id="FlashRangeAddr">
		<target>TMS320C28XX</target>
		<name>Range: </name>
		<valueType>String</valueType>
		<defaultValue>0x80000-0x81FFF</defaultValue>
		<enableIf>
			<property id="FlashRangeToggle">1</property>
		</enableIf>
	</property>
	
	<property id="FlashCombineText">
		<target>TMS320C28XX</target>
		<name>Note: any unwritten memory locations between sections will be filled with 1 when sections are combined,
and ECC will be programmed if AutoEccGeneration is enabled.</name>
		<valueType>Text</valueType>
	</property>
	
	<property id="FlashCombineText2">
		<target>TMS320C28XX</target>
		<name>Note2: Program sections on different pages will not be combined. If AutoEccGeneration is enabled, 
page 0 and page 1 sections cannot share the same 64-bit aligned memory.</name>
		<valueType>Text</valueType>
	</property>
	
	<property id="FlashAlignCombineToggle">
		<target>TMS320C28XX</target>
		<name>Combine Sections during Program Load to increase performance</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
	</property>
	
	<property id="FlashAlignmentSize">
		<target>TMS320C28XX</target>
		<name>Specify maximum size for combining sections: 0x</name>
		<valueType>String</valueType>
		<defaultValue>1000</defaultValue>
		<disableIf>
			<property id="FlashCombineSections">0</property>
		</disableIf>
		<hidden />
	</property>
	
	<property id="FlashDownloadSetting">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>ChoiceList</valueType>
		<values>
			<value>Erase and Program</value>
			<value>Program Only</value>
			<value>Verify Only</value>
		</values>
		<defaultValue>Erase and Program</defaultValue>
	</property>
	
	<property id="FlashBlankCheckToggle">
		<target>TMS320C28XX</target>
		<name>Perform Blank Check before loading data to Flash memory</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
	</property>
	
	<property id="FlashVerifySetting">
		<target>TMS320C28XX</target>
		<name>Verify Flash after Program</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
	</property>
	
	<property id="FlashEraseSelection">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>ChoiceList</valueType>
		<values>
			<value>Entire Flash</value>
			<value>Necessary Sectors Only (for Program Load)</value>
			<value>Selected Sectors Only</value>
		</values>
		<defaultValue>Entire Flash</defaultValue>
	</property>
	
	<property id="FlashC28Bank0Sector0">
		<target>TMS320C28XX</target>
		<name>Sector 0 (0x80000 - 0x80FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector1">
		<target>TMS320C28XX</target>
		<name>Sector 1 (0x81000 - 0x81FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector2">
		<target>TMS320C28XX</target>
		<name>Sector 2 (0x82000 - 0x82FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector3">
		<target>TMS320C28XX</target>
		<name>Sector 3 (0x83000 - 0x83FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector4">
		<target>TMS320C28XX</target>
		<name>Sector 4 (0x84000 - 0x84FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector5">
		<target>TMS320C28XX</target>
		<name>Sector 5 (0x85000 - 0x85FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector6">
		<target>TMS320C28XX</target>
		<name>Sector 6 (0x86000 - 0x86FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector7">
		<target>TMS320C28XX</target>
		<name>Sector 7 (0x87000 - 0x87FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector8">
		<target>TMS320C28XX</target>
		<name>Sector 8 (0x88000 - 0x88FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector9">
		<target>TMS320C28XX</target>
		<name>Sector 9 (0x89000 - 0x89FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector10">
		<target>TMS320C28XX</target>
		<name>Sector 10 (0x8A000 - 0x8AFFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector11">
		<target>TMS320C28XX</target>
		<name>Sector 11 (0x8B000 - 0x8BFFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector12">
		<target>TMS320C28XX</target>
		<name>Sector 12 (0x8C000 - 0x8CFFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector13">
		<target>TMS320C28XX</target>
		<name>Sector 13 (0x8D000 - 0x8DFFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector14">
		<target>TMS320C28XX</target>
		<name>Sector 14 (0x8E000 - 0x8EFFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashC28Bank0Sector15">
		<target>TMS320C28XX</target>
		<name>Sector 15 (0x8F000 - 0x8FFFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
	<property id="FlashEraseToggle0Button">
		<target>TMS320C28XX</target>
		<name>Toggle Bank 0 Selection</name>
		<valueType>Button</valueType>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
		<action>for( var i = 0; DPActiveDebugSession.options.optionExist( "FlashC28Bank0Sector" + i ); ++i ) { DPActiveDebugSession.options.setBoolean( "FlashC28Bank0Sector" + i, !DPActiveDebugSession.options.getBoolean( "FlashC28Bank0Sector" + i ) ); }</action>
	</property>
	
	<property id="FlashEraseButton">
		<target>TMS320C28XX</target>
		<name>Erase Flash</name>
		<valueType>Button</valueType>
		<disableIf>
			<property id="FlashEraseSelection">Necessary Sectors Only (for Program Load)</property>
		</disableIf>
		<action>DPActiveDebugSession.flash.performOperation("Erase")</action>
	</property>

	<property id="FlashCRCMainValue">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>String</valueType>
		<defaultValue></defaultValue>
		<readOnly>1</readOnly>
	</property>

	<property id="FlashCRCECCValue">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>String</valueType>
		<defaultValue></defaultValue>
		<readOnly>1</readOnly>
	</property>

	<property id="FlashCRCOTPValue">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>String</valueType>
		<defaultValue></defaultValue>
		<readOnly>1</readOnly>
	</property>

	<property id="FlashCRCOTPECCValue">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>String</valueType>
		<defaultValue></defaultValue>
		<readOnly>1</readOnly>
	</property>
	
	<property id="FlashCalChecksumButton">
		<target>TMS320C28XX</target>
		<name>Calculate Checksum</name>
		<valueType>Button</valueType>
		<action>DPActiveDebugSession.flash.performOperation("CalculateChecksum")</action>
	</property>
	
	<property id="SecurityText">
		<target>TMS320C28XX</target>
		<name>Note: All security values are located in One-time programmable (OTP) memory. 
Take caution prior to programming these values, as each bit can be programmed from 1 to 0 only once.</name>
		<valueType>Text</valueType>
	</property>
	
	<property id="Z1B0LINKPOINTER1">
		<target>TMS320C28XX</target>
		<name>B0-Z1OTP-LINKPOINTER1 (0x78000)(32 bits) 0x</name>
		<valueType>String</valueType>
		<defaultValue>--</defaultValue>
	</property>
	
