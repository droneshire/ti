	<property id="Z1OTPBOOTCTRLButton">
		<target>TMS320C28XX</target>
		<name>Program OTPBOOTCTRL</name>
		<valueType>Button</valueType>
		<action>DPActiveDebugSession.flash.performOperation("Z1OTPBOOTCTRLProgram")</action>
	</property>
	
