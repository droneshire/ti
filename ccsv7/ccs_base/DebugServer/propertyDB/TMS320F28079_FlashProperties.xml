			<child>Z1OTPSECLOCKGroup</child>
			<child>Z1OTPBOOTCTRLGroup</child>
			<child>Z1CSMPSWDGroup</child>
			<child>Z1GRABGroup</child>
			<child>Z1EXEONLYGroup</child>
			<child>Z1ProgramAllButton</child>
		</children>
	</group>
	
	<group id="Z1LINKPOINTERGroup">
		<name>LINKPOINTER</name>
