<Decoder>
	<DecoderName>IsNonEmulatable</DecoderName>
	<BitfieldMask>0x40</BitfieldMask>
	<BitfieldShift>6</BitfieldShift>
	<!-- convert from Emulatable = 1 to NonEmulatable = 1 by xoring value with 0x40 -->
	<XorWithRegValue>0x40</XorWithRegValue>
</Decoder>
<Decoder>
	<DecoderName>IsSecure</DecoderName>
	<BitfieldMask>0x80</BitfieldMask>
	<BitfieldShift>7</BitfieldShift>
	<!-- convert from NonSecure = 1 to Secure = 1 by xoring value with 0x80 -->
	<XorWithRegValue>0x80</XorWithRegValue>
</Decoder>
<Decoder>
	<DecoderName>IsAccessID_LOCAL</DecoderName>
	<BitfieldMask>0x100</BitfieldMask>
	<BitfieldShift>8</BitfieldShift>
</Decoder>
<Decoder>
	<DecoderName>IsAccessID_AIDX</DecoderName>
	<BitfieldMask>0x200</BitfieldMask>
	<BitfieldShift>9</BitfieldShift>
</Decoder>
<Decoder>
	<DecoderName>IsAccessID_CPU</DecoderName>
	<BitfieldMask>0x0FC00</BitfieldMask>
	<BitfieldShift>10</BitfieldShift>
</Decoder>
<Decoder>
	<DecoderName>L1DLruToWayMap</DecoderName>
	<BitfieldMask>0x01</BitfieldMask>
	<BitfieldShift>0</BitfieldShift>
</Decoder>
<Decoder>
	<DecoderName>L2LruToWayMap</DecoderName>
	<BitfieldMask>0x07</BitfieldMask>
	<BitfieldShift>0</BitfieldShift>
	<Multiplier>1.0</Multiplier>
	<AddToProduct>0</AddToProduct>
	<!--
				From the GEM_UMC spec, table 46:
				LRU Interpretation

				b0 b1 b2 (MRU to LRU order)
				0  0  0		0, 1, 2, 3
				0  0  1		0, 1, 3, 2
				0  1  0		1, 0, 2, 3
				0  1  1		1, 0, 3, 2
				1  0  0		2, 3, 0, 1
				1  0  1		3, 2, 0, 1
				1  1  0		2, 3, 1, 0
				1  1  1		3, 2, 1, 0

				reversing this to get b2-b1 as an index:
				index:  LRU way
					0	3
					1	1
					2	3
					3	0
					4	2
					5	1
					6	2
					7	0
				-->
	<LookupTable>
		<NumIndexValues>8</NumIndexValues>
		<Array index="0">3</Array>
		<Array index="1">1</Array>
		<Array index="2">3</Array>
		<Array index="3">0</Array>
		<Array index="4">2</Array>
		<Array index="5">1</Array>
		<Array index="6">2</Array>
		<Array index="7">0</Array>
	</LookupTable>
</Decoder>
<!-- GetCoreNumber extracts the device number for the device core from the DNUM control register -->
<Decoder>
	<DecoderName>GetCoreNumber</DecoderName>
	<XorWithRegValue>0x0</XorWithRegValue>
	<BitfieldMask>0x0F</BitfieldMask>
	<BitfieldShift>0</BitfieldShift>
	<Multiplier>1.0</Multiplier>
	<AddToProduct>0</AddToProduct>
	<IsTrueIfResultIsZero>false</IsTrueIfResultIsZero>
</Decoder>
<!-- GetCoreNumber uses the core number from the DNUM control register as an index into a lookup table
     that specifies the base global address for that core -->
<Decoder>
	<DecoderName>GetGlobalAdrsForThisCore</DecoderName>
	<XorWithRegValue>0x00000000</XorWithRegValue>
	<BitfieldMask>0x0F</BitfieldMask>
	<BitfieldShift>0</BitfieldShift>
	<Multiplier>1.0</Multiplier>
	<AddToProduct>0</AddToProduct>
	<LookupTable>
		<NumIndexValues>16</NumIndexValues>
		<Array index="0">0x010000000</Array>
		<Array index="1">0x011000000</Array>
		<Array index="2">0x012000000</Array>
		<Array index="3">0x013000000</Array>
		<Array index="4">0x014000000</Array>
		<Array index="5">0x015000000</Array>
		<Array index="6">0x016000000</Array>
		<Array index="7">0x017000000</Array>   
		<Array index="8">0x018000000</Array>
		<Array index="9">0x019000000</Array>
		<Array index="10">0x01A000000</Array>
		<Array index="11">0x01B000000</Array>
		<Array index="12">0x01C000000</Array>
		<Array index="13">0x01D000000</Array>
		<Array index="14">0x01E000000</Array>
		<Array index="15">0x01F000000</Array>   
	</LookupTable>
</Decoder>
