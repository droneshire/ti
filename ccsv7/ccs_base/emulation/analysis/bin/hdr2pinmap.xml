<?xml version="1.0"?>

<!-- This file can be used whether or not a Drishti is in the system.  If there's no Drishti, 
    don't specify any DRISHTIPIN-NUM fields.  If you do, they will be ignored.  
    This file defines the connectivity between the target header and the ports of the FPGA and Drishti. -->

<!-- Possible port ID's are: FPGAINPUT-NUM, FPGAOUTPUT-NUM, and DRISHTIPIN-NUM. -->

<HDR2PORT-MAPPING version="1.0">
	<MAP> <!-- EMU0 Pin map--> 
		<HDRPIN-NUM>B14</HDRPIN-NUM> <!-- Header pin number-->
		<DRISHTIPIN-NUM>0x00</DRISHTIPIN-NUM> <!-- Drishti pin number-->
	</MAP>
	<MAP> <!-- EMU1 Pin map--> 
		<HDRPIN-NUM>C14</HDRPIN-NUM> <!-- Header pin number-->
		<DRISHTIPIN-NUM>0x01</DRISHTIPIN-NUM> <!-- Drishti pin number-->
	</MAP>
